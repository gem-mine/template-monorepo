{
  "files.associations": {
    "**/content/**/*": "nunjucks",
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "content/packages/**/*": true,
    "*.log": true,
  }
}